<h1>BarChart</h1>

<div id="chart"></div>

<script type="text/javascript">
var chart = c3.generate({
  bindto: '#chart',
  data: {
    columns: [
      ['data1', 30, 200, 100]
    ],
    type: 'bar'
  },
  // Category Axis
  // https://c3js.org/samples/categorized.html
  axis: {
    x: {
      type: 'category',
      categories: ['Cambridge', 'London', 'Oxford']
    }
  }
});
</script>
