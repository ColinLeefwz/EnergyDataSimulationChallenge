<h1>Energies</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Label</th>
      <th>House</th>
      <th>Year</th>
      <th>Month</th>
      <th>Temperature</th>
      <th>Daylight</th>
      <th>Energy production</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @energies.each do |energy| %>
      <tr>
        <td><%= energy.id %></td>
        <td><%= energy.label %></td>
        <td><%= energy.house_id %></td>
        <td><%= energy.year %></td>
        <td><%= energy.month %></td>
        <td><%= energy.temperature %></td>
        <td><%= energy.daylight %></td>
        <td><%= energy.energy_production %></td>
        <td><%= link_to 'Show', energy %></td>
      </tr>
    <% end %>
  </tbody>
</table>
