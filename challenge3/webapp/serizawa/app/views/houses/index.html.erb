<h1>House_data</h1>

<table>
  <thead>
    <tr>
      <th>firstname</th>
      <th>lastname</th>
      <th>city</th>
      <th>num_of_people</th>
      <th>has_child</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @houses.each do |house| %>
      <tr>
        <td><%= house.firstname %></td>
        <td><%= house.lastname %></td>
        <td><%= house.city %></td>
        <td><%= house.num_of_people %></td>
        <td><%= house.has_child %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>