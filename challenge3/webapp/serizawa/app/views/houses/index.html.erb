<h1>Houses_data</h1>
<%= link_to 'Energies_data', energies_path %>
<br>
<br>
<%= link_to 'New', new_house_path %>



<table>
  <thead>
    <tr>
      <th>firstname</th>
      <th>lastname</th>
      <th>city</th>
      <th>num_of_people</th>
      <th>has_child</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @houses.each do |house| %>
      <% cache house do %>
        <tr>
          <td><%= house.firstname %></td>
          <td><%= house.lastname %></td>
          <td><%= house.city %></td>
          <td><%= house.num_of_people %></td>
          <td><%= house.has_child %></td>
          <td><%= link_to 'Show', house %></td>
          <td><%= link_to 'Edit', edit_house_path(house) %></td>
          <td><%= link_to 'Destroy', house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>